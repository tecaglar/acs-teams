{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "AllowedHosts": "*",
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "TenantId": "<<TENANT-ID>>",
    "ClientId": "<<AZURE-Client-ID>>",
    "ClientSecret": "<<AZURE-Client-SECRET>>",
    "AuthUrl": "/oauth2/v2.0/token"
  },
  "Bot": {
    "AppId": "<<AZURE-APP-ID>>",
    "AppSecret": "<<AZURE-APP-SECRET>>",
    "PlaceCallEndpointUrl": "https://graph.microsoft.com/v1.0",
    "BotBaseUrl": "<<APP-SERVICE-BASE-URL>>"
  },
  "MicrosoftAppId": "<<AZURE-APP-ID>>",
  "MicrosoftAppPassword": "<<AZURE-APP-SECRET>>",
  "endpoint": "<<COSMOS-DB-URI>>",
  "authKey": "<<COSMOS-DB-AUTH-KEY>>",
  "database": "<<COSMOS-DATABASE-NAME>>",
  "collection": "<<COSMOS-COLLECTION-NAME>>",
  "ChannelId": "<<TEAMS-CHANNEL-ID-WHERE-CONVERSATION-NEED-TO-BE-HAPPEN>>",
  "TeamsServiceUrl": "<<BOT-SERVICE-URL>>",
  "ResourceConnectionString": "ACS Resource Connection String",
  "BaseUrl": "<<APP-SERVICE-BASE-URL>>",
  "ApplicationInsights": {
    "ConnectionString": "<<ApplicationInsights-ConnectionString>>"
  },
  "UserIds": "<<Teams user's aadids (comma seperated values), who will get the calls from bot.",
  "UserId": "<<Object Id of the User to whom the online meeting application policy is granted>>"
}